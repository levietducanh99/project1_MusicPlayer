<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yourapp.MusicApp.controller.LibraryController" spacing="10" alignment="CENTER">
    <Label text="Library - Add Song" />

    <!-- Thêm ListView để hiển thị danh sách bài hát -->
    <ListView fx:id="songListView" prefHeight="200" prefWidth="300" />

    <!-- Thêm các nút điều hướng và chức năng -->
    <Button text="Thêm Bài Hát" onAction="#handleAddSong" />
    <Button text="Go Back to Home" onAction="#handleGoBackToHome" />
</VBox>
