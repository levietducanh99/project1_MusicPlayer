<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="261.0" prefWidth="55.0" style="-fx-background-color: #f57c64;" styleClass="sidebar" stylesheets="@../css/abc.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yourapp.myfirstMusicApp.uiComponent.CustomMenu">
   <children>
      <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="46.0" prefWidth="34.0" styleClass="Library-button" text="Go to Playlist">
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../pic/Add%20Album.png" />
               </image>
            </ImageView>
         </graphic>
         <stylesheets>
            <URL value="@../css/abc.css" />
            <URL value="@../css/IconStyling.css" />
         </stylesheets></Button>
      <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="30.0" prefWidth="43.0" styleClass="Library-button" text="Go to Favourite">
         <stylesheets>
            <URL value="@../css/abc.css" />
            <URL value="@../css/IconStyling.css" />
         </stylesheets>
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../pic/Love%20Circled.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="goToPlayerButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleGoToPlayerButton" prefHeight="26.0" prefWidth="43.0" styleClass="Library-button" text="Go to Player">
         <stylesheets>
            <URL value="@../css/abc.css" />
            <URL value="@../css/IconStyling.css" />
         </stylesheets>
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../pic/Player.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="30.0" prefWidth="41.0" styleClass="Library-button" text="Go to History">
         <stylesheets>
            <URL value="@../css/abc.css" />
            <URL value="@../css/IconStyling.css" />
         </stylesheets>
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../pic/History.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="goToLibraryButton" alignment="CENTER" contentDisplay="CENTER" onAction="#handleGoToLibraryButton" prefHeight="53.0" prefWidth="46.0" styleClass="Library-button" text="Go to Library">
         <stylesheets>
            <URL value="@../css/abc.css" />
            <URL value="@../css/IconStyling.css" />
         </stylesheets>
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../pic/MusicLibrary.png" />
               </image>
            </ImageView>
         </graphic></Button>
